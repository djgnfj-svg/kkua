# 데이터베이스 설정
DATABASE_URL=postgresql://postgres:password@db:5432/kkua_db

# Redis 설정
REDIS_URL=redis://redis:6379/0

# 보안 설정
SECRET_KEY=your-secret-key-change-in-production-very-long-random-string
JWT_SECRET=your-jwt-secret-change-in-production-also-very-long-random-string

# 환경 설정
ENVIRONMENT=development
DEBUG=true

# CORS 설정
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# 세션 설정
SESSION_TIMEOUT=86400

# 로깅 설정
LOG_LEVEL=INFO